{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "parser": "babel-eslint",
  "rules": {
      "semi": "error"
  },
  "env": {
      "browser": true,
      "node": true,
      "mocha": true
  },
  "plugins": [
      "react"
  ],
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
  ]
}